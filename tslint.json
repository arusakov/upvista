{
    "extends": "tslint:latest",
    "rules": {
        "interface-name": [
            false
        ],
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            true, "multiline"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    }
}